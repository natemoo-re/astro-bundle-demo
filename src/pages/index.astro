---
import Basic from "../components/Basic.astro";
import Cached from "../components/Cached.astro";

export const prerender = true;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Astro</title>
  </head>
  <body>
    <h1>Astro</h1>
    <Basic data={{ a: 0, b: true, c: [] }} />
    <Cached data={{ a: 0, b: true, c: [] }} />
    <Cached data={{ a: 1, b: true, c: [] }} />
    <Cached data={{ a: 1, b: true, c: [] }} />
    <Cached data={{ a: 2, b: true, c: [] }} />
    <Cached data={{ a: 2, b: true, c: [] }} />

    <form id="foo" method="post" action="/form">
      <input type="text" name="name" />
      <button type="submit">Submit</button>
    </form>
  </body>
</html>
