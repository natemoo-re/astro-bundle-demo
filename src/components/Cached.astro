---
import { hash, Store } from "astro:store";

interface Props {
  data: any;
}

const { data } = Astro.props;
const h = hash(JSON.stringify(data));

const store = new Store("image");
const asset = await store.putWithCache(`${h}.json`, async () => {
  await new Promise((res) => setTimeout(res, 2000));
  return JSON.stringify(data, null, 2);
});
---

<pre>{asset}</pre>

<script type="module" define:vars={{ asset }}>
  const foo = await fetch(asset).then((res) => res.json());
  console.log(foo);
</script>
